<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addon id="plugin.video.myclouds"
  name="MyClouds"
  version="1.3.11"
  provider-name="ytan">
  <requires>
      <import addon="script.module.beautifulsoup4" version="4.3.2" optional="false"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="myclouds.py">
  <provides>image audio video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <platform>all</platform>
    <description>A cloud drive media browser, now supports 115, baiduyun and XunleiLixian, Kodi 14.0 Helix above required.</description>
    <description lang="zh">云服务媒体浏览器，目前支持百度云、115网盘和迅雷离线，需要运行在Kodi 14.0 Helix及以上版本的系统。</description>
    <disclaimer></disclaimer>
  </extension>
</addon>
